{
    "name": "hps/heartland-magento2",
    "description": "Heartland Secure Submit to Portico gateway",
    "repositories": {
        "hps": {
            "type": "git",
            "url": "https://github.com/hps/heartland-php.git"
        }
    },
    "require": {
        "hps/heartland-php": "^2.8.15",
        "magento/module-config": "100.1.*",
        "magento/module-store": "101.0.*",
        "magento/module-checkout": "100.1.*",
        "magento/module-catalog": "101.0.*",
        "magento/module-sales": "100.1.*",
        "magento/module-customer": "100.1.*",
        "magento/module-payment": "100.1.*",
        "magento/module-quote": "100.1.*",
        "magento/module-backend": "100.1.*",
        "magento/module-directory": "100.1.*",
        "magento/module-theme": "100.1.*",
        "magento/framework": "100.1.*"
    },
    "type": "magento2-module",
    "version": "100.1.2",
    "license": [
        "proprietary"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "HPS\\Heartland\\": ""
        }
    }
}
