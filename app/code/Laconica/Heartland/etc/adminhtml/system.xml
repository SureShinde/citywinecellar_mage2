<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <section id="payment">
            <group id="heartland_section">
                <group id="heartland">
                    <field id="disable_heartland_frontend" translate="label" type="select" sortOrder="1"
                           showInDefault="1" showInWebsite="1"
                           showInStore="0">
                        <label>Disable For Frontend</label>
                        <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                    </field>
                </group>
            </group>
        </section>
    </system>
</config>