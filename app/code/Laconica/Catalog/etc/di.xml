<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Magento\Catalog\Block\Product\View\Attributes"
                type="Laconica\Catalog\Block\Product\View\Attributes"/>

    <type name="Magento\Framework\Console\CommandList">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="import_amasty_rates" xsi:type="object">Laconica\Catalog\Console\Command\ImportAmastyRates</item>
                <item name="la_catalog_update_product" xsi:type="object">Laconica\Catalog\Console\Command\UpdateProduct</item>
                <item name="la_catalog_update_product_visibility" xsi:type="object">Laconica\Catalog\Console\Command\UpdateProductVisibility</item>
            </argument>
        </arguments>
    </type>
</config>