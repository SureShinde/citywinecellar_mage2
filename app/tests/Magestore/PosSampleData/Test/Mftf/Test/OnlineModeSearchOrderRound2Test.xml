    <test name="OnlineModeSearchOrderByIncrementIdGetting1ResultR2">
        <annotations>
            <features value="Online mode: Search order"/>
            <stories value="Online mode: Search order"/>
            <title value="Online mode: Search order by increment Id and Get 1 result (R2)"/>
            <description value="Online mode: Search order by increment Id and Get 1 result (R2)"/>
            <severity value="CRITICAL"/>
            <group value="Stable"/>
            <group value="Stable-Online"/>
            <group value="Stable-Online-Order"/>
        </annotations>
        <before>
            <actionGroup ref="PosOnCheckoutPage" stepKey="PosOnCheckoutPage"/>
            <amOnPage url="{{PosOrderHistoryPage.url}}" stepKey="navigateToPosOrderHistory"/>
        </before>

        <actionGroup ref="SearchOrderAndCountResultActionGroup" stepKey="searchOrder">
            <argument name="keyword" value="pos_stable_order_1_result.keyword_increment_id"/>
            <argument name="numberOrders" value="pos_stable_order_1_result.numberOrders"/>
        </actionGroup>
    </test>

