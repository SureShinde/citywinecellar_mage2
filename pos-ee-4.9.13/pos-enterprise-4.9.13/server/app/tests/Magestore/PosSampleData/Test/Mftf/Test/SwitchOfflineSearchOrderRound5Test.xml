    <test name="SwitchOfflineSearchOrderByIncrementIdGetting1ResultR5">
        <annotations>
            <features value="Offline mode: Search order"/>
            <stories value="Offline mode: Search order"/>
            <title value="Offline mode: Search order by increment Id and Get 1 result (R5)"/>
            <description value="Offline mode: Search order by increment Id and Get 1 result (R5)"/>
            <severity value="CRITICAL"/>
            <group value="Stable"/>
            <group value="Stable-Offline"/>
            <group value="Stable-Offline-Order"/>
        </annotations>
        <before>
            <actionGroup ref="PosOnCheckoutPage" stepKey="PosOnCheckoutPage"/>
            <amOnPage url="{{PosOrderHistoryPage.url}}" stepKey="navigateToPosOrderHistory"/>
        </before>

        <actionGroup ref="SearchOrderAndCountResultActionGroup" stepKey="searchOrder">
            <argument name="keyword" value="pos_stable_order_1_result.keyword_increment_id"/>
            <argument name="numberOrders" value="pos_stable_order_1_result.numberOrders"/>
        </actionGroup>
    </test>

