    <test name="OnlineSearchCustomerStabilityByEmail1">
        <annotations>
            <features value="Customer"/>
            <stories value="Search Customer Stable Online"/>
            <title value="Search Customer Stable By Email - Number 1"/>
            <description value="Search Customer Stable By Email - Number 1"/>
            <severity value="MAJOR"/>
            <group value="Stable"/>
            <group value="Stable-Online"/>
            <group value="Stable-Online-Customer"/>
        </annotations>
        <before>
            <actionGroup ref="PosOnCheckoutPage" stepKey="PosOnCheckoutPage"/>
        </before>

        <actionGroup ref="SwitchOnlineModeActionGroup" stepKey="SwitchToOnlineMode" />
        <actionGroup ref="ClearCart" stepKey="ClearCart" />

        <actionGroup ref="SearchCustomerByKeywordActionGroup" stepKey="SearchCustomerByKeywordActionGroup">
            <argument name="keyword" value="stability_customer_00000002.email"/>
            <argument name="customer" value="stability_customer_00000002"/>
        </actionGroup>
    </test>

